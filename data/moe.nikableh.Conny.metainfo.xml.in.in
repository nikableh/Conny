<?xml version="1.0" encoding="UTF-8"?>
<component type="desktop-application">
  <id>@app-id@</id>
  <metadata_license>CC0</metadata_license>
  <project_license>GPL-3.0-only</project_license>
  <name>Conny</name>
  <summary>Use OpenVPN configs from GUI</summary>
  <description>
    <p>A GUI application for managing OpenVPN profiles and active connections.</p>
  </description>
  <screenshots>
    <screenshot type="default">
      <!-- FIXME Set the project git url and replace the screenshot -->
      <image>https://gitlab.gnome.org/bilelmoussaoui/gtk-rust-template/raw/master/data/resources/screenshots/screenshot1.png</image>
      <caption>Main window</caption>
    </screenshot>
  </screenshots>
  <url type="homepage">https://github.com/nikableh/Conny</url>
  <url type="bugtracker">https://github.com/nikableh/Conny/issues</url>
  <url type="vcs-browser">https://gitlab.gnome.org/bilelmoussaoui/gtk-rust-template</url>
  <url type="translate">https://github.com/nikableh/Conny/-/tree/master/po</url>
  <content_rating type="oars-1.1" />
  <releases>
    <release version="0.1.0" date="2099-01-01" />
  </releases>
  <developer id="@nika@nikableh.moe">
    <name translate="yes">Nika</name>
  </developer>
  <branding>
    <color type="primary" scheme_preference="light">#c2ba5b</color>
    <color type="primary" scheme_preference="dark">#993d3d</color>
  </branding>
  <update_contact>nika@nikableh.moe</update_contact>
  <translation type="gettext">@gettext-package@</translation>
  <launchable type="desktop-id">@app-id@.desktop</launchable>
</component>
